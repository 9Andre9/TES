{
  "name": "matpay",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --mode cypress",
    "lint": "vue-cli-service lint",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "mkdir:reports": "mkdir reports || true",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "precopy:reports": "npm run mkdir:reports",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage --nycrc-path nycrc.vue.json",
    "test:e2e:ci": "vue-cli-service test:e2e --mode cypress-ci --headless",
    "test:e2e:wsl": "sudo /etc/init.d/dbus start &> /dev/null; DISPLAY=$(cat /etc/resolv.conf | grep nameserver | awk '{print $2; exit;}'):0.0 vue-cli-service test:e2e --mode cypress"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "core-js": "^3.6.5",
    "dinero.js": "^1.9.1",
    "lodash": "^4.17.21",
    "seamless-scroll-polyfill": "^2.1.6",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/vue": "^3.1.0",
    "@cypress/webpack-preprocessor": "^5.11.0",
    "@types/bootstrap": "^5.1.6",
    "@types/dinero.js": "^1.9.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@typescript-eslint/scope-manager": "^4.18.0",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.1",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-plugin-router": "^5.0.1",
    "@vue/cli-plugin-typescript": "^5.0.1",
    "@vue/cli-plugin-unit-jest": "^5.0.1",
    "@vue/cli-plugin-vuex": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "babel-jest": "^27.0.6",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^9.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.1.0",
    "jest-extended": "^1.2.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5"
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  }
}
